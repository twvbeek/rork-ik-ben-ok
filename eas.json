{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.11.0",
      "npm": {
        "install": "--legacy-peer-deps"
      },
      "env": {
        "EXPO_NO_BUN": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "22.11.0",
      "npm": {
        "install": "--legacy-peer-deps"
      },
      "env": {
        "EXPO_NO_BUN": "1"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "22.11.0",
      "npm": {
        "install": "--legacy-peer-deps"
      },
      "env": {
        "EXPO_NO_BUN": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}